    "jest": "^21.2.1",
    "raf-polyfill": "^1.0.0",
    "redux-devtools": "^3.4.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-hot-server-middleware": "^0.3.0"
  }
