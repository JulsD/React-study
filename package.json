{
  "name": "react-study",
  "version": "0.0.1",
  "description": "trainig mass",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint",
    "build": "webpack",
    "build:watch": "webpack -d --watch",
    "serve": "webpack-dev-server --progress --hot --colors --config ./webpack.config.js",
    "build:prod": "webpack -p",
    "serve:prod": "node dist/server.js",
    "prestart": "npm i",
    "start": "npm run build:prod & npm run serve:prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./src/tests/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulsD/React-study.git"
  },
  "author": "DJuls",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JulsD/React-study/issues"
  },
  "homepage": "https://github.com/JulsD/React-study#readme",
  "dependencies": {
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.2.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.3.2",
